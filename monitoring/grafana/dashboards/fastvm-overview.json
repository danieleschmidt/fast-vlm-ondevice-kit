{
  "dashboard": {
    "id": null,
    "title": "FastVLM On-Device Kit - Overview",
    "tags": ["fastvm", "ml", "mobile"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Conversion Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(conversion_duration_seconds[5m])",
            "legendFormat": "{{model_variant}} - {{quantization}}"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Inference Latency by Device",
        "type": "timeseries",
        "targets": [
          {
            "expr": "inference_latency_ms",
            "legendFormat": "{{device}} - {{model_variant}}"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0,
            "max": 1000
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Model Sizes",
        "type": "stat",
        "targets": [
          {
            "expr": "converted_model_size_mb",
            "legendFormat": "{{model_variant}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB",
            "max": 1000
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(error_count[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.01
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "System Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "system_memory_percent",
            "legendFormat": "Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "system_cpu_percent",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Inference Accuracy Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "inference_accuracy",
            "legendFormat": "{{model_variant}} - {{device}}"
          }
        ],
        "yAxes": [
          {
            "label": "Accuracy",
            "min": 0.5,
            "max": 1.0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Model Conversion Queue",
        "type": "timeseries",
        "targets": [
          {
            "expr": "conversion_queue_size",
            "legendFormat": "Queue Size"
          },
          {
            "expr": "rate(conversion_queue_processed[5m])",
            "legendFormat": "Processing Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Recent Errors",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"fastvm-app\"} |= \"ERROR\"",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1
  }
}